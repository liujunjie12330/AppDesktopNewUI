<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="30dp"
    android:layout_height="30dp"
    xmlns:tools="http://schemas.android.com/tools">
    <View
        android:id="@+id/selected_bg"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:layout_gravity="center"
        android:background="@drawable/paster_color_select_bg" />
    <cn.synway.module_sdirector_ui.whiteboard.ui.custom.CircleView
        android:id="@+id/paster_color"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="1px"
        android:layout_marginStart="1px"
        android:layout_gravity="center"/>
    <ImageView
        android:id="@+id/iv_button"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="1px"
        android:layout_marginStart="1px"
        android:layout_gravity="center"
        android:visibility="gone"
        android:background="@drawable/bg_circle_white_inset"
        android:src="@drawable/wb_ic_transparent"
        tools:ignore="ContentDescription" />
</FrameLayout>