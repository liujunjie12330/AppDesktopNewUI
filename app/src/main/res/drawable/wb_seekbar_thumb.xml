<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="10dp"
        android:height="10dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/deep_blue"/>
        </shape>
    </item>
    <item
        android:left="1dp"
        android:top="1dp"
        android:width="8dp"
        android:height="8dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/white"/>
        </shape>
    </item>
</layer-list>